{
  "name": "sijoitussimulaattori",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test:watch": "jest --watch",
    "start:dummy": "node ./dummy-server/index.js"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(js)$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "<rootDir>/tools/tests-setup.js"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/ios/Pods/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native)/)"
    ],
    "testEnvironment": "jsdom",
    "cacheDirectory": ".jest/cache"
  },
  "dependencies": {
    "await-to-js": "^2.0.1",
    "axios": "^0.18.0",
    "i18next": "^12.0.0",
    "react": "16.6.0-alpha.8af6728",
    "react-native": "^0.57.4",
    "react-native-elements": "^1.0.0-beta7",
    "react-native-firebase": "^5.0.0",
    "react-native-languages": "^3.0.1",
    "react-native-size-matters": "^0.1.4",
    "react-native-vector-icons": "^6.0.2",
    "react-navigation": "^2.18.1",

    "react-redux": "^5.0.7",
    "redux": "^4.0.1",
    "redux-axios-middleware": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.14",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/i18n-js": "^3.0.1",
    "@types/i18next": "^11.9.3",
    "@types/jest": "^23.3.5",
    "@types/jsdom": "^12.2.0",
    "@types/node": "^10.12.0",
    "@types/react": "^16.4.18",
    "@types/react-native": "^0.57.7",
    "@types/react-redux": "^6.0.9",
    "@types/react-test-renderer": "^16.0.3",
    "@types/react-navigation": "^2.0.24",
    "babel-core": "^6.26.3",
    "babel-jest": "23.6.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "i18n-js": "^3.0.11",
    "jest": "23.6.0",
    "jest-environment-jsdom": "^23.4.0",
    "jest-enzyme": "^7.0.0",
    "jsdom": "^12.2.0",
    "metro-react-native-babel-preset": "0.48.1",
    "react-dom": "^16.5.2",
    "react-native-mock-render": "^0.1.2",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.6.0-alpha.8af6728",
    "redux-devtools-extension": "^2.13.5",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.3"
  }
}
